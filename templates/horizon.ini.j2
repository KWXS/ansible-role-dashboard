[program:horizon-home.{{ work_domain }}]
directory={{ ansible_env.HOME }}/{{ work_dir }}/home.{{ work_domain }}/
command=/usr/local/opt/{{ php_version }}/bin/php artisan horizon

process_name=%(program_name)s_%(process_num)02d
autostart=true
autorestart=true
user={{ ansible_user }}
numprocs=1
redirect_stderr=true
stdout_logfile={{ ansible_env.HOME }}/.log/horizon-home.{{ work_domain }}.log
